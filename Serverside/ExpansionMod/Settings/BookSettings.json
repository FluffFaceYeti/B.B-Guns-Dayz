    "RuleCategories": [
        {
            "CategoryName": "General P1",
            "Rules": [
                {
                    "RuleParagraph": "A)",
                    "RuleText": "No Stream sniping."
                },
                {
                    "RuleParagraph": "B)",
                    "RuleText": "No racism, sexism, or hate speech of any kind."
                },
                {
                    "RuleParagraph": "C)",
                    "RuleText": "Abuse of exploits will result in a Ban."
                },
                {
                    "RuleParagraph": "D)",
                    "RuleText": "Max group size is 5!"
                },
                {
                    "RuleParagraph": "E)",
                    "RuleText": "Combat loggers will be Banned."
                }
            ]
        },
        {
            "CategoryName": "General P2",
            "Rules": [
                {
                    "RuleParagraph": "A)",
                    "RuleText": "We ask players to interact before they shoot however this does not apply in military zones."
                },
                {
                    "RuleParagraph": "B)",
                    "RuleText": "Any player caught harassing or bullying another player on the server will Banned."
                },
                {
                    "RuleParagraph": "C)",
                    "RuleText": "DO NOT broadcast music to the server at anytime, We are a Streamer friendly server."
                },
                {
                    "RuleParagraph": "D)",
                    "RuleText": "Do not message admins directly for general questions. We have a ticket system for that."
                },
                {
                    "RuleParagraph": "E)",
                    "RuleText": "Speaking poorly of others on streams, in chats, or elsewhere will not be tolerated."
                }
            ]
        },
        {
            "CategoryName": "Base Rules P1",
            "Rules": [
                {
                    "RuleParagraph": "A)",
                    "RuleText": "No base without a flag."
                },
                {
                    "RuleParagraph": "B)",
                    "RuleText": "1 flag per group."
                },
                {
                    "RuleParagraph": "C)",
                    "RuleText": "No building within 1km of Traders or heavy Military zones."
                },
                {
                    "RuleParagraph": "D)",
                    "RuleText": "No hoarding vehicles."
                },
                {
                    "RuleParagraph": "E)",
                    "RuleText": "Do not deploy a flag without the intention of building a base."
                }
            ]
        },
        {
            "CategoryName": "Base Rules P2",
            "Rules": [
                {
                    "RuleParagraph": "A)",
                    "RuleText": "Inactive bases will be removed after 14 days."
                },
                {
                    "RuleParagraph": "B)",
                    "RuleText": "Max three cars per base."
                },
                {
                    "RuleParagraph": "C)",
                    "RuleText": "Do not build on main roads."
                },
                {
                    "RuleParagraph": "D)",
                    "RuleText": "No tents inside tents."
                },
                {
                    "RuleParagraph": "E)",
                    "RuleText": "Perimiter traps are allowed."
                }
            ]
        },
        {
            "CategoryName": "Trader Rules P1",
            "Rules": [
                {
                    "RuleParagraph": "A)",
                    "RuleText": "No camping traders."
                },
                {
                    "RuleParagraph": "B)",
                    "RuleText": "No stealing inside the trader zones."
                },
                {
                    "RuleParagraph": "C)",
                    "RuleText": "Do not follow people out of the traders to kill them."
                },
                {
                    "RuleParagraph": "D)",
                    "RuleText": "Do not use Megaphones or hot mic within the traders."
                },
                {
                    "RuleParagraph": "E)",
                    "RuleText": "Vehicles left inside the traders will be removed."
                }
            ]
        },
        {
            "CategoryName": "Trader Rules P2",
            "Rules": [
                {
                    "RuleParagraph": "A)",
                    "RuleText": "Do not use Trader or Safezones to escape PVP situations."
                },
                {
                    "RuleParagraph": "B)",
                    "RuleText": "Do not loiter in Trader zones."
                },
                {
                    "RuleParagraph": "C)",
                    "RuleText": "No stashes within or near Trader zones, They will be removed."
                },
                {
                    "RuleParagraph": "D)",
                    "RuleText": "No toxic behaviour in the Trader zones. Leave your fights outside!"
                },
                {
                    "RuleParagraph": "E)",
                    "RuleText": "Private trades are allowed."
                },
                {
                    "RuleParagraph": "F)",
                    "RuleText": "No stealing in the Trader Zones. This includes touching other players vehicles in anyway!."
                }
            ]
        },
                {
                    "CategoryName": "Report a player!",
